LAYERS {

LAYER {
	1-18 {
        NAME: 'land'
        TYPE: POLYGON
        GROUP: 'default'
        STATUS: ON
        PROJECTION {
            1-3 {{
				'init=epsg:4326'
            }}
        	4-18 {{
				'init=epsg:3857'
			}}
        }
		DATA {
			1-3: "../pdata/osm/TM_WORLD_BORDERS-0.3.shp"
            4-6: "../pdata/osm/shoreline_300.shp"
            7-18: "../pdata/osm/processed_p.shp"
		}
        CLASS {
            STYLE {
            	COLOR { 
					1-13: 250 250 250
                    14-18: 250 250 250
                }
    			OUTLINECOLOR: 250 250 250
            }
        }
    }
}
LAYER {
	2-18 {
        NAME: 'admin_fr'
        TYPE: POLYGON
        @connection
        @layerconfig
      	DATA {
         	2-18:  "geometry from (select geometry,osm_id, name as name,admin_level from osm_admin ) as foo using unique osm_id using srid=3857"
   		}
        LABELITEM: "name"
        1-18{
        CLASS {
            STYLE {
                OUTLINECOLOR: 200 200 200
                SIZE {
                    1-3: 0.3
					5-18: 0.9
                }
                PATTERN {{
					5.0 5.0
				}}
            }
            
            7-18 {
                LABEL{
                  TYPE: TRUETYPE
                  PARTIALS: FALSE
                  ANTIALIAS: true
                  FONT: 'sc'
                  ANGLE: FOLLOW
                  SIZE: 8
                  COLOR: '#564A3D'
                  OUTLINECOLOR: 255 255 255
                  OUTLINEWIDTH: 1
                  REPEATDISTANCE: 100
                  MINDISTANCE: 500
               }
        	}
          }
        }
    }
}

LAYER {
	 6-18 {
        NAME: 'waterarea'
        TYPE: POLYGON
        @connection
        @layerconfig
        DATA {
	6-10: "geometry from (select geometry,osm_id ,name,type from osm_waterareas_gen1) as foo using unique osm_id using srid=3857"
	11-18: "geometry from (select geometry,osm_id ,name,type from osm_waterareas) as foo using unique osm_id using srid=3857"
		}    	
    	LABELITEM: 'name'
        CLASSITEM: 'type'
        CLASS {
	          EXPRESSION: "riverbank"
              STYLE {
