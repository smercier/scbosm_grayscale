##Shields
LAYER {
	9-18 {
        NAME: 'shields'
        TYPE: LINE
        @connection
        @layerconfig
        DATA {
        	1-7: "geometry from (select osm_id,geometry,name, ref,(regexp_matches(ref, '([a-z,A-Z]+)? ?([0-9]+)[^;]*', 'g'))[1] as shield_class,(regexp_matches(ref, '([a-z,A-Z]+)? ?([0-9]+)[^;]*', 'g'))[2] as shield_no,type,tunnel,bridge from osm_roads_gen0 where type = 'motorway' order by z_order asc, st_length(geometry) asc) as foo using unique osm_id using srid=3857" 
		8-18: "geometry from (select osm_id,geometry,name, ref,(regexp_matches(ref, '([a-z,A-Z]+)? ?([0-9]+)[^;]*', 'g'))[1] as shield_class,(regexp_matches(ref, '([a-z,A-Z]+)? ?([0-9]+)[^;]*', 'g'))[2] as shield_no,type,tunnel,bridge from osm_roads where type in ('motorway', 'trunk', 'primary') order by z_order asc, st_length(geometry) asc) as foo using unique osm_id using srid=3857"
		}
        FILTER: (length('[shield_no]') != 0 )
    	LABELITEM: 'shield_no'
        CLASSITEM: 'shield_class'
        CLASS {
			9-18 {
				EXPRESSION: ('[type]' = 'motorway')
                TEXT : '[shield_class][shield_no]'
                LABEL {
					@labelconfig_shields
					SIZE: 7
                	##COLOR: 255 255 255
                    STYLE {
                        GEOMTRANSFORM: 'labelpoly'
                        COLOR: '#F2F2F2'
                        OUTLINECOLOR: 0 0 0
                        WIDTH:0.3
					}
				}
			}
		}
    	CLASS {
			9-18 {
                TEXT : '[shield_class][shield_no]'
                LABEL {
					@labelconfig_shields
					SIZE: 6
                	##COLOR: 255 255 255
                    STYLE {
                        GEOMTRANSFORM: 'labelpoly'
                        COLOR: '#F2F2F2'
                        OUTLINECOLOR: 0 0 0
                        WIDTH:0.3
					}
				}
			}
		}
	}
}            
   


##Aeroways
LAYER {
	11-18 {
        NAME: 'aeroways'
        TYPE: LINE
        @connection
        @layerconfig
        DATA: 'geometry from (select geometry, osm_id, type from osm_aeroways) as foo using unique osm_id using srid=3857'
		CLASSITEM: 'type'
        CLASS {
			EXPRESSION: 'runway'
            STYLE {
				COLOR: 180 180 180
                WIDTH {
					1-11: 1
                    12: 2
                    13: 4
                    14: 9
                    15: 15
                    16: 30
                    17-18: 40
				}
            	LINECAP: BUTT
                LINEJOIN: MITER
			}
        	STYLE {
                COLOR: 80 80 80
               	WIDTH {
					1-15: 0
                    16-18: 1
				}
				PATTERN {{
					2 2
				}}
            	LINECAP: BUTT
			}
        	
		}
    	CLASS {
			EXPRESSION: 'taxiway'
            STYLE {
				COLOR: 180 180 180
                WIDTH {
					1-10: 0
                    11-12: 0.5
                    13: 1
                    14: 2
                    15: 4
                    16: 8
                    17-18: 12
				}
			}
		}
	}
}
     
